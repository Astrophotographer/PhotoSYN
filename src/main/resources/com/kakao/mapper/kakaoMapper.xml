<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kakao.mapper.KakaoRepo">
    <insert id="kakaoInsert" >
        insert into kakao(num, name, email, image)
        values (#{id}, #{nickname}, #{email}, #{thumbnail_image})
    </insert>
<!--
    <select id="findKakao" resultType="kdto">
        select *
        from kakao
        where name = #{name}
          and email = #{email}
    </select>
-->
</mapper>